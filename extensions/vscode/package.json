{
  "name": "bloc",
  "displayName": "bloc",
  "description": "Support for the bloc library and provides tools for effectively creating blocs for both Flutter and AngularDart apps.",
  "version": "2.0.0",
  "publisher": "FelixAngelov",
  "bugs": {
    "url": "https://github.com/felangel/bloc/issues",
    "email": "felangelov@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/felangel/bloc"
  },
  "homepage": "https://bloclibrary.dev",
  "engines": {
    "vscode": "^1.31.0"
  },
  "categories": [
    "Snippets",
    "Programming Languages"
  ],
  "keywords": [
    "dart",
    "flutter",
    "angulardart",
    "bloc",
    "state-management"
  ],
  "icon": "assets/logo.png",
  "activationEvents": [
    "onCommand:extension.new-bloc",
    "workspaceContains:**/pubspec.yaml"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.new-bloc",
        "title": "Bloc: New Bloc",
        "icon": "assets/logo.png"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.new-bloc",
          "group": "blocGroup@1",
          "when": "explorerResourceIsFolder"
        }
      ]
    },
    "snippets": [
      {
        "language": "dart",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/change-case": "^2.3.1",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.121",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^2.2.42",
    "@types/node": "^12.7.8",
    "@types/rimraf": "^2.0.2",
    "@types/semver": "^6.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.5.1",
    "vscode": "^1.1.33"
  },
  "dependencies": {
    "change-case": "^3.1.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "mkdirp": "^0.5.1",
    "semver": "^6.3.0"
  }
}
